,GAAR,CAAYF,QAAZ;AACA,aAAI,CAACP,OAAL,GAAe,KAAf;;AACA,aAAI,CAACU,aAAL;AACD,OANH,EAOGC,KAPH,CAOS,UAACC,KAAD,EAAW;AAChBJ,eAAO,CAACC,GAAR,CAAYG,KAAZ;AACA,aAAI,CAACZ,OAAL,GAAe,KAAf;AACD,OAVH;AAWA,WAAKF,KAAL,GAAa,EAAb;AACD,KAnBM;AAoBPe,iBApBO,yBAoBOC,EApBP,EAoBW;AAAA;;AAChB;AACA,WAAKd,OAAL,GAAe,IAAf;AACAL,WAAI,CACDoB,MADH,CACU,uCAAuCD,EADjD,EAEGR,IAFH,CAEQ,UAACC,QAAD,EAAc;AAClBC,eAAO,CAACC,GAAR,CAAYF,QAAZ;;AACA,cAAI,CAACG,aAAL;;AACA,cAAI,CAACV,OAAL,GAAe,KAAf;AACD,OANH,EAOGW,KAPH,CAOS,UAACC,KAAD,EAAW;AAChBJ,eAAO,CAACC,GAAR,CAAYG,KAAZ;AACA,cAAI,CAACZ,OAAL,GAAe,KAAf;AACD,OAVH;AAWD,KAlCM;AAmCPgB,eAnCO,uBAmCKlB,KAnCL,EAmCYgB,EAnCZ,EAmCgB;AAAA;;AACrB;AACA,WAAKd,OAAL,GAAe,IAAf;AACAL,WAAI,CACDsB,GADH,CACO,uCAAuCH,EAD9C,EACkDhB,KADlD,EAEGQ,IAFH,CAEQ,YAAM;AACV,cAAI,CAACI,aAAL;;AACA,cAAI,CAACV,OAAL,GAAe,KAAf;AACD,OALH,EAMGW,KANH,CAMS;AAAA,eAAO,MAAI,CAACX,OAAL,GAAe,KAAtB;AAAA,OANT;AAOD,KA7CM;AA8CPU,iBA9CO,2BA8CS;AAAA;;AACd,WAAKV,OAAL,GAAe,IAAf;AACAL,WAAI,CACDuB,GADH,CACO,mCADP,EAEGZ,IAFH,CAEQ,UAACC,QAAD,EAAc;AAClB,cAAI,CAACR,UAAL,GAAkBQ,QAAQ,CAACV,IAA3B;AACA,cAAI,CAACG,OAAL,GAAe,KAAf;AACD,OALH,EAMGW,KANH,CAMS;AAAA,eAAO,MAAI,CAACX,OAAL,GAAe,KAAtB;AAAA,OANT;AAOD;AAvDM,GATI;AAkEbmB,SAAO,EAAE,mBAAY;AACnB,SAAKT,aAAL;AACD;AApEY,CAAf","names":["axios","name","data","tarea","listTareas","loading","methods","agregarTarea","nombre","estado","post","then","response","console","log","obtenerTareas","catch","error","eliminarTarea","id","delete","editarTarea","put","get","created"],"sourceRoot":"","sources":["D:\\Proyectos Github\\Aplicacion Vue_NetCore_EFC\\Aplicacion FrondAndBack-Git\\src\\components\\Tarea.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h1 class=\"display-4 text-center\">Listado de Tareas</h1>\r\n    <hr />\r\n    <div class=\"row\">\r\n      <div class=\"col-lg-8 offset-lg-2\">\r\n        <div class=\"card mt-4\">\r\n          <div class=\"card-body\">\r\n            <div class=\"input-group\">\r\n              <input\r\n                type=\"text\"\r\n                v-model=\"tarea\"\r\n                class=\"form-control form-control-lg\"\r\n                placeholder=\"Agregar tarea\"\r\n              />\r\n              <div class=\"input-group-append\">\r\n                <button\r\n                  class=\"btn btn-success btn-lg\"\r\n                  v-on:click=\"agregarTarea()\"\r\n                >\r\n                  Agregar\r\n                </button>\r\n              </div>\r\n            </div>\r\n            <br />\r\n            <div class=\"text-center\">\r\n              <div\r\n                v-if=\"loading\"\r\n                class=\"spinner-border text-success\"\r\n                role=\"status\"\r\n              >\r\n                <span class=\"visually-hidden\"></span>\r\n              </div>\r\n            </div>\r\n            <h3 v-if=\"listTareas.length == 0\" class=\"text-center\">\r\n              No hay tareas para realizar\r\n            </h3>\r\n            <ul v-if=\"!loading\" class=\"list-group\">\r\n              <li\r\n                v-for=\"(tarea, index) of listTareas\"\r\n                :key=\"index\"\r\n                class=\"list-group-item d-flex justify-content-between\"\r\n              >\r\n                <span\r\n                  class=\"cursor\"\r\n                  v-bind:class=\"{ 'text-success': tarea.estado }\"\r\n                  v-on:click=\"editarTarea(tarea, tarea.id)\"\r\n                >\r\n                  <i\r\n                    v-bind:class=\"[\r\n                      tarea.estado ? 'fas fa-check-circle' : 'far fa-circle',\r\n                    ]\"\r\n                  >\r\n                  </i>\r\n                </span>\r\n                {{ tarea.nombre }}\r\n                <span\r\n                  class=\"text-danger cursor\"\r\n                  v-on:click=\"eliminarTarea(tarea.id)\"\r\n                >\r\n                  |<i class=\"fas fa-trash-alt\"></i>\r\n                </span>\r\n              </li>\r\n            </ul>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Tarea\",\r\n  data() {\r\n    return {\r\n      tarea: \"\",\r\n      listTareas: [],\r\n      loading: false,\r\n    };\r\n  },\r\n  methods: {\r\n    agregarTarea() {\r\n      const tarea = {\r\n        nombre: this.tarea,\r\n        estado: false,\r\n      };\r\n      this.loading = true;\r\n      axios\r\n        .post(\"https://localhost:44314/api/Tarea/\", tarea)\r\n        .then((response) => {\r\n          console.log(response);\r\n          this.loading = false;\r\n          this.obtenerTareas();\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n          this.loading = false;\r\n        });\r\n      this.tarea = \"\";\r\n    },\r\n    eliminarTarea(id) {\r\n      // this.listTareas.splice(index, 1);\r\n      this.loading = true;\r\n      axios\r\n        .delete(\"https://localhost:44314/api/Tarea/\" + id)\r\n        .then((response) => {\r\n          console.log(response);\r\n          this.obtenerTareas();\r\n          this.loading = false;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n          this.loading = false;\r\n        });\r\n    },\r\n    editarTarea(tarea, id) {\r\n      // this.listTareas[index].estado = !tarea.estado;\r\n      this.loading = true;\r\n      axios\r\n        .put(\"https://localhost:44314/api/Tarea/\" + id, tarea)\r\n        .then(() => {\r\n          this.obtenerTareas();\r\n          this.loading = false;\r\n        })\r\n        .catch(() => (this.loading = false));\r\n    },\r\n    obtenerTareas() {\r\n      this.loading = true;\r\n      axios\r\n        .get(\"https://localhost:44314/api/Tarea\")\r\n        .then((response) => {\r\n          this.listTareas = response.data;\r\n          this.loading = false;\r\n        })\r\n        .catch(() => (this.loading = false));\r\n    },\r\n  },\r\n  created: function () {\r\n    this.obtenerTareas();\r\n  },\r\n};\r\n</script>\r\n\r\n<style>\r\n.cursor {\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"]}]}